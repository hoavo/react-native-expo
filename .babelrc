{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./src"],
            "alias": {
              "test": "./test",
              "underscore": "lodash"
            }
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./src"],
            "alias": {
              "test": "./test",
              "underscore": "lodash"
            }
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "module-resolver",
          {
            "root": ["./src"],
            "alias": {
              "test": "./test",
              "underscore": "lodash"
            }
          }
        ]
      ]
    }
  }
}
